{
  "name": "web-image-tools",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --hot src/index.tsx",
    "start": "NODE_ENV=production bun src/index.tsx",
    "build": "bun run build.ts",
    "build2": "bun build ./src/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*' --public-dir=public"
  },
  "dependencies": {
    "@pixi/react": "^8.0.3",
    "bun-plugin-tailwind": "^0.0.15",
    "colorthief": "^2.6.0",
    "exifr": "^7.1.3",
    "pixi.js": "^8.14.0",
    "react": "^19",
    "react-dom": "^19",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/colorthief": "^2.6.0",
    "@types/react": "^19",
    "@types/react-dom": "^19"
  }
}
